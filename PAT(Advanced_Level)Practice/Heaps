#include<iostream>
#include<string>
using namespace std;
int v[1006], n;

void postorder(int ind) {
    if(ind>n) return;
    postorder(ind * 2);
    postorder(ind * 2 + 1);
    printf("%d%s", v[ind], ind==1? "\n":" ");
}

int main(void) {
    int i, j, k, m, z;
    cin>>m>>n;
    for(i = 0; i < m; i++) {
        int max=9, min = 9;
        for(j = 1; j <= n; j++)
            scanf("%d", &v[j]);
        for(j = 2; j <= n; j++) {
            if(v[j] > v[j/2]) max = 0;
            if(v[j] < v[j/2]) min = 0; 
        }
        if(max==9) cout<<"Max Heap"<<endl;
        else if(min==9) cout<<"Min Heap"<<endl;
        else cout<<"Not Heap"<<endl;
        postorder(1);
    }
    return 0;
}
